{
  "name": "@mfukushim/map-traveler-mcp",
  "version": "0.0.1",
  "type": "module",
  "bin": {
    "map-traveler-mcp": "build/esm/mapTraveler.js"
  },
  "files": [
    "build/esm/",
    "assets",
    "drizzle/"
  ],
  "packageManager": "pnpm@9.10.0",
  "license": "MIT",
  "description": "Virtual traveler library for MCP",
  "repository": {
    "type": "git",
    "url": "https://github.com/mfukushim/map-traveler-mcp.git"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist",
    "registry": "https://npm.pkg.github.com",
    "@mfukushim:registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "codegen": "build-utils prepare-v2",
    "build": "pnpm build-esm && pnpm build-annotate",
    "build-esm": "tsc -b tsconfig.build.json",
    "build-annotate": "babel build/esm --plugins annotate-pure-calls --out-dir build/esm --source-maps",
    "build:shx": "pnpm build-esm && shx chmod +x build/esm/*.js",
    "check": "tsc -b tsconfig.json",
    "test": "vitest",
    "coverage": "vitest --coverage",
    "inspector": "pnpm build && cmd /c \"set CLIENT_PORT=5174 && npx @modelcontextprotocol/inspector build/esm/index.js\""
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@atproto/api": "^0.13.23",
    "@effect/platform": "latest",
    "@effect/platform-node": "latest",
    "@effect/sql": "^0.22.3",
    "@effect/sql-sqlite-node": "^0.23.2",
    "@libsql/client": "^0.14.0",
    "@modelcontextprotocol/sdk": "^1.0.4",
    "@pixai-art/client": "^0.4.0",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.38.2",
    "effect": "^3.12.0",
    "form-data": "^4.0.1",
    "geolib": "^3.3.4",
    "jimp": "^1.6.0",
    "openai": "^4.75.0",
    "sharp": "^0.33.5",
    "transparent-background": "^1.5.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.25.2",
    "@babel/plugin-transform-export-namespace-from": "^7.24.7",
    "@babel/plugin-transform-modules-commonjs": "^7.24.8",
    "@effect/build-utils": "^0.7.7",
    "@effect/language-service": "^0.1.0",
    "@effect/vitest": "^0.16.0",
    "@types/node": "^22.5.2",
    "@types/ws": "^8.5.13",
    "babel-plugin-annotate-pure-calls": "^0.4.0",
    "drizzle-kit": "^0.30.1",
    "shx": "^0.3.4",
    "tsx": "^4.17.0",
    "typescript": "^5.6.2",
    "vitest": "^2.0.5"
  },
  "effect": {
    "generateExports": {
      "include": [
        "**/*.ts"
      ]
    },
    "generateIndex": {
      "include": [
        "**/*.ts"
      ]
    }
  }
}
